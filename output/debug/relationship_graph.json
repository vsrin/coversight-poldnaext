    {
      "id": "element_declarations_3_cbe6ec19",
      "type": "SUB_LIMIT",
      "section_id": "declarations",
      "text": "Limits of Insurance: $1,000,000 Each Occurrence",
      "metadata": {
        "subtype": "Monetary Limit",
